{
    "temp-dir": "./coverage/.nyc-output",
    "report-dir": "./coverage",
    "all": true,
    "check-coverage": true,
    "extension": [".ts"],
    "statements": 75,
    "functions": 75,
    "branches": 75,
    "lines": 75,
    "reporter": ["html", "lcov", "text"],
    "include": ["src/**/*.ts", "test/**/*.ts"]
}
